{
  "name": "@pescarte/boia",
  "description": "Boia design system",
  "version": "0.1.0",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Zoey de Souza Pessanha",
      "email": "zoey.spessanha@outlook.com",
      "url": "https://github.com/zoedsoupe"
    }
  ],
  "engines": {
    "node": "^16.17"
  },
  "files": [
    ".vscode",
    "dist",
    "public/",
    "theme/",
    "cli/bin",
    "cli/build",
    "*.ts"
  ],
  "bin": {
    "boia-cli": "./cli/bin/boia-cli"
  },
  "main": "./dist/boia.umd.js",
  "module": "./dist/boia.es.js",
  "exports": {
    ".": {
      "import": "./dist/boia.es.js",
      "require": "./dist/boia.umd.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./windi.config.ts": "./windi.config.ts"
  },
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "dev": "vite --port 3333 --host",
    "build:lib": "vue-tsc --noEmit && vite build",
    "build:lib:ts": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "preview": "vite preview",
    "lint": "eslint src/ --resolve-plugins-relative-to .",
    "lint:fix": "eslint src/ --fix --resolve-plugins-relative-to .",
    "boia-cli": "cli/bin/boia-cli",
    "test": "vitest run",
    "test:w": "vitest watch --silent ",
    "test:coverage": "vitest run --silent --coverage",
    "storybook": "start-storybook -p 6006 --ci",
    "build-storybook": "build-storybook",
    "cli:clean-build": "rm -rf ./cli/build",
    "cli:compile": "tsc -p ./cli",
    "cli:copy-templates": "copyfiles -f ./cli/src/templates/* ./cli/build/templates",
    "cli:build": "yarn cli:clean-build && yarn cli:compile && yarn cli:copy-templates",
    "prepare": "husky install",
    "commit": "cz",
    "pre-commit": "./.husky/run-staged-tests.js"
  },
  "dependencies": {
    "@vueuse/core": "^9.3.0",
    "gluegun": "latest",
    "maska": "^1.5.0",
    "vee-validate": "^4.6.6",
    "vue": "^3.2.40"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.1",
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@iconify/json": "^2.1.101",
    "@storybook/addon-a11y": "^6.5.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/theming": "^6.5.10",
    "@storybook/vue3": "^6.5.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^6.6.1",
    "@types/jest-axe": "^3.5.4",
    "@types/node": "^17.0.41",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vitest/coverage-c8": "^0.22.1",
    "@vitest/ui": "^0.22.1",
    "babel-loader": "^8.2.5",
    "c8": "^7.12.0",
    "cli-spinner": "^0.2.10",
    "commitizen": "^4.2.4",
    "copyfiles": "^2.4.1",
    "critters": "^0.0.16",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "eslint-plugin-vuejs-accessibility": "^1.2.0",
    "husky": "^8.0.1",
    "jest-axe": "^5.0.1",
    "jsdom": "^20.0.0",
    "sass": "^1.50.0",
    "storybook-addon-designs": "^6.3.1",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-property-sort-order-smacss": "^9.0.0",
    "stylelint-order": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "unplugin-icons": "^0.14.8",
    "vite": "^3.0.9",
    "vite-plugin-inspect": "^0.6.0",
    "vite-plugin-windicss": "^1.8.7",
    "vitest": "^0.23.2",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.40.13",
    "windicss": "^3.5.6"
  },
  "resolutions": {
    "ejs": "^3.1.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peapescarte/boia"
  },
  "keywords": [
    "vue 3",
    "Components",
    "PEA Pescarte",
    "boia",
    "windicss",
    "design system",
    "vue",
    "vue.js",
    "nuxt",
    "nuxt 3"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
